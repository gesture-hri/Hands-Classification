{
    "dataset": {
        "labels": ["rock", "paper", "scissors"],
        "paths": ["data/rock.npy", "data/paper.npy", "data/scissors.npy"]
    },

    "models": {
        "names": ["logistic_regression", "sgd_classifier", "knn_classifier", "random_forest"],
        "paths": [
            "models/logistic_regression.pkl", "models/sgd_classifier.pkl", 
            "models/knn_classifier.pkl", "models/random_forest.pkl"
        ],
        "test_size": 0.2,
        "random_state": 42
    },
    
    "preprocessor": {
        "name": "mediapipe_preprocessor",
        "path": "preprocessors/mediapipe_preprocessor.pkl"
    }
}